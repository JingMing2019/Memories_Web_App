{
  "name": "TooTasty",
  "version": "1.0.0",
  "description": "Restaurant ratings reviews application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "keywords": [
    "rating",
    "review",
    "restaurant",
    "full stack",
    "MERN"
  ],
  "author": "Anqi Luo, Jiaxi Wen, Jingyu Luo, Kexin Qiu",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.15",
    "@redux-devtools/extension": "^3.2.3",
    "@reduxjs/toolkit": "^1.9.0",
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.2.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "mongoose-unique-array": "^0.4.2",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.3",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "yelp-fusion": "^3.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15"
  }
}
